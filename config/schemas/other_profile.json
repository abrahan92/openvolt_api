{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "birthdate": {
      "type": "string",
      "format": "date",
      "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
    },
    "phone_number": {
      "type": "string"
    }
  },
  "required": [
    "birthdate",
    "phone_number"
  ],
  "additionalProperties": false
}